 const sanitizeTitle = function(title){
    return  title.replace(/[^\u3131-\uD79D a-zA-Z0-9 \u00C0-\u00FF \u0153]/g, ' ')
                 .split(' ')
                 .filter(word => word.length >= minWordLength);
}
 const findKeywords = function(){
    let title = document.querySelector("meta[name='news_keywords']");

    if(title !== null) title = title.getAttribute("content");
    if(title !== null) title = sanitizeTitle(title);
    if((title === null || title.length === 0) && document.getElementsByTagName("h1")[0] !== undefined)
        title = sanitizeTitle(document.getElementsByTagName("h1")[0].textContent);
    
    return title;
}

let searchParams = {    
    'fields': 'id',
    'sort': sort,
    'limit': 1,
    'search': findKeywords().sort((a, b) => b.length - a.length).slice(0, maxWordSearch).join(' ')
};
if(!searchInPlaylist) searchParams.private = 0;
if(!searchInPlaylist) searchParams.longer_than = 0.35; //21sec
